public class Solution {
    public int[] solve(int[] A) {
        int B[] = new int[A.length];
         
        for(int i = 0 ; i<A.length ; i++){
          
            int count = 0;
            for(int j= 0 ; j<A.length ; j++){
            if(A[i]==A[j]){
               count++;
            }
            }
            B[i]=count;
            
            
        }
        return B;
    }
 }

